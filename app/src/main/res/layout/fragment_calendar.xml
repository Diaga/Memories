<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <CalendarView
        android:id="@+id/calendarView"
        android:layout_width="341dp"
        android:layout_height="299dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="8dp"
        android:layout_marginTop="84dp"
        android:layout_marginEnd="8dp"
        android:weekNumberColor="?android:textColor"
        android:dateTextAppearance="?android:textColor"
        android:unfocusedMonthDateColor="?android:textColor"
        android:selectedWeekBackgroundColor="?android:textColor"
        android:weekSeparatorLineColor="?android:textColor"
        android:focusedMonthDateColor="?android:textColor"
        android:weekDayTextAppearance="?android:textColor"
        android:foregroundTint="@color/bottom_nav_selector_change_color"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>